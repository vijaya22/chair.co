{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "chair.co",
  "description": "This is a Chrome extension built with React and TypeScript",
  "action": {
    "default_popup": "index.html",
    "default_title": "chair.co"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "js": ["main.js"],
      "matches": ["*://*.google.com/*"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["*"],
      "matches": ["*://*.google.com/*"]
    }
  ]
}
